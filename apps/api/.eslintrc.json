{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*", "Observable<any>"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "camelcase": ["error", { "allow": ["setId_userId", "leitnerSetId_cardId", "jwt_decode"] }]
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "no-control-regex": "off"
      }
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
